    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.2",
    "raw-loader": "0.5.1",
    "style-loader": "0.23.1",
