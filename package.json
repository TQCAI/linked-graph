    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "url-loader": "1.1.2",
    "webpack": "4.25.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
